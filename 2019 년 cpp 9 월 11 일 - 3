// 변수  초기화 - uniform initialization
//--------------------------------------------------------------------------

#include <iostream>
#include "save.h"


using namespace std;

int main() {
	//초기화란 메모리가 확보될때 처음 들어갈 값을 쓰는 동작이다.
	int a;	// int에 해당하는 만큼의 메모리를 요청한다 그 할당된 메모리를 d라고 하기로 한다
	int b = 0;	//명시적인 초기화
	int c = int(0);	//복사 초기화
	int d = b;  // 복사 초기화
	int e{};	//unigform initialization의 디폴트 초기화
	int g{ 0 };	//unigform initialization
	//int f(); 는 안됨 변수 선언일뿐이다


	save("소스.cpp");


}
