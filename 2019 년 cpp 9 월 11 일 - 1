#include <iostream>
#include <random>
#include "save.h"

using namespace std;

//c++ 랜덤값 생성성
//랜덤엔진으로 랜덤값 생성
//확률 분포로 원하는 통계특성 설정

int main() {

	random_device rd;
	mt19937 eng{ rd() };
	uniform_int_distribution<int> uid(1, 6);

	for (int i = 0; i < 10; ++i) {
		cout << eng() << endl;
	}

	save("소스.cpp");


}
